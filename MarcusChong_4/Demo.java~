import java.io.File;
import java.io.PrintWriter;

public class Demo
{
	public static void main(String[] args)
	{
		PriterWriter outputStream = null;
		try
		{
			outputStream = newPrintWriter(new FileOutPutStream("test.txt"));
		}
		catch (FileNotFounException e)
		{]
			System.out.print("Error opening the file test.txt");
			System.exit(0);
		}
		System.out.println("Writing to file");
		
		outputStream.println("ATGC");
		outputStream.println("CGTA");
		
		outputStream.close();
		
		System.out.println("End or Program");
		
		
	}
}
